@model ClpQrColoring.Models.ArCharacters._RainbowPartialViewModel

<div id="divRainbow">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xs-6" style="padding: 0;">
                <img id="imgEarth" alt="earth"
                     class="img-responsive pull-left"
                     src="@Model.ImgEarthSrc"
                     srcset="@Model.ImgEarthSrcset" />
            </div>
            <div class="col-xs-6" style="padding: 0;">
                <img id="imgRainbowKid" alt="rainbow kid"
                     class="img-responsive pull-right"
                     src="@Model.ImgRainbowKidSrc"
                     srcset="@Model.ImgRainbowKidSrcset" />
            </div>
        </div>
    </div>
</div>